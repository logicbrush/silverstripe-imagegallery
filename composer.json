{
		"name": "logicbrush/silverstripe-imagegallery",
		"description": "A module for the SilverStripe CMS which allows you to organize images in a gallery format.",
		"homepage": "https://www.logicbrush.com/specialities/silverstripe-development/",
		"type": "silverstripe-vendormodule",
		"keywords": [
				"silverstripe",
				"gallery"
		],
		"license": "BSD-3-Clause",
		"authors": [{
				"name": "John Learn",
				"email": "jflearn@logicbrush.com"
		}],
		"require": {
				"silverstripe/cms": "^4.0",
				"silverstripe/asset-admin": "*",
				"undefinedoffset/sortablegridfield": "*",
				"jonom/focuspoint": "*",
				"silverstripe/widgets": "^2.0",
				"bummzack/sortablefile": "^2.1"
		},
		"extra": {
				"expose": [
						"thirdparty",
						"javascript",
						"images"
				]
		},
		"require-dev": {
				"phpunit/phpunit": "^5.7"
		},
		"autoload": {
				"psr-4": {
						"Logicbrush\\ImageGallery\\": "src/"
				}
		}
}
